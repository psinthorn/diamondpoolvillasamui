      </div>

      <div class="form-group">
        <%= form.label :instagram %>
        <%= form.text_field :instagram, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :main_img %>
        <%= form.text_field :main_img, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= form.label :thumb_img %>
